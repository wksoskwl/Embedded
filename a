#include <LiquidCrystal_I2C.h>
LiquidCrystal_I2C lcd(0x3F, 16, 2);

byte k[] = {
  B11101,
  B00101,
  B00101,
  B00101,
  B00000,
  B00111,
  B00101,
  B00111
};

byte m[] = {
  B11101,
  B10101,
  B10101,
  B11101,
  B00000,
  B00100,
  B00100,
  B00111
};

byte s[] = {
  B00100,
  B01010,
  B10001,
  B00000,
  B11111,
  B00100,
  B01010,
  B00100
};

void setup() {  
  lcd.init();
  lcd.backlight();
  lcd.createChar(0, k);
  lcd.createChar(1, m);
  lcd.createChar(2, s);
}

void loop()
{
  lcd.write(0);
  lcd.write(1);
  lcd.write(2);
  delay(100);
  lcd.clear();
}
